extern printf(format: *i8, ...) : void;
extern memcpy(dst : *void, src :*void, n : u64): *void;

def main(argc : i32, argv : *i8): i32
    let str : i8[13];                       # stack allocated value
    str = memcpy(str, "Hell  World!", 12);
    str[4] = 'o';
    str[12] = '\0';
    printf("Test \"%s\" end", str);
    return 0;
end

